{
    "version": 2,
    "framework": null,
    "installCommand": "npm install",
    "buildCommand": "npm run build",
    "outputDirectory": ".",
    "functions": {
        "api/index.php": {
            "runtime": "vercel-php@0.7.2"
        }
    },
    "routes": [
        {
            "src": "/build/(.*)",
            "dest": "/public/build/$1"
        },
        {
            "src": "/(css|js|images|image|fonts|lottie|favicon.ico|robots.txt)(.*)",
            "dest": "/public/$1$2"
        },
        {
            "src": "/(.*)",
            "dest": "/api/index.php"
        }
    ],
    "env": {
        "APP_ENV": "production",
        "APP_DEBUG": "false",
        "APP_KEY": "base64:kmEqBTgtna88WEdUN08O8lWi9ePlExs81hDCdHoMImE=",
        "WEB_KEY": "base64:kmEqBTgtna88WEdUN08O8lWi9ePlExs81hDCdHoMImE=",
        "LOG_CHANNEL": "stderr",
        "DB_CONNECTION": "sqlite",
        "DB_DATABASE": ":memory:",
        "SESSION_DRIVER": "cookie",
        "CACHE_STORE": "array",
        "FILESYSTEM_DISK": "public",
        "LARAVEL_STORAGE_PATH": "/tmp",
        "VIEW_COMPILED_PATH": "/tmp/views"
    }
}